{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [],
    "pipeline": {
        "build": {
            "outputs": [
                ".next/**/*",
                "!.next/cache/**",
                ".vercel/**/*",
                "dist/**/*"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "start": {
            "dependsOn": [
                "build"
            ],
            "persistent": true
        },
        "lint": {},
        "dev": {
            "dependsOn": [
                "build"
            ],
            "persistent": true
        },
        "clean": {},
        "publish": {
            "dependsOn": [
                "build"
            ]
        }
    }
}